{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1",
            "headers": {
                "Strict-Transport-Security": "max-age=31536000; includeSubDomains"
            }
        },
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1",
            "headers": {
                "Strict-Transport-Security": "max-age=31536000; includeSubDomains"
            }
        },
        {
            "src": "/js/(.*)",
            "dest": "/js/$1",
            "headers": {
                "Strict-Transport-Security": "max-age=31536000; includeSubDomains"
            }
        },
        {
            "src": "/style/(.*)",
            "dest": "/style/$1",
            "headers": {
                "Strict-Transport-Security": "max-age=31536000; includeSubDomains"
            }
        },
        {
            "src": "/engine/(.*)",
            "dest": "/engine/$1",
            "headers": {
                "Strict-Transport-Security": "max-age=31536000; includeSubDomains"
            }
        },
        {
            "src": "/(.*)",
            "dest": "app.py",
            "headers": {
                "Strict-Transport-Security": "max-age=31536000; includeSubDomains"
            }
        }
    ],
    "github": {
        "silent": true
    }
} 