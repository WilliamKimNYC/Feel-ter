{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1"
        },
        {
            "src": "/js/(.*)",
            "dest": "/js/$1"
        },
        {
            "src": "/style/(.*)",
            "dest": "/style/$1"
        },
        {
            "src": "/engine/(.*)",
            "dest": "/engine/$1"
        },
        {
            "src": "/favicon.ico",
            "dest": "/favicon.ico"
        },
        {
            "src": "/manifest.json",
            "dest": "/manifest.json"
        },
        {
            "src": "/service-worker.js",
            "dest": "/service-worker.js"
        },
        {
            "src": "/(.*)",
            "dest": "/app.py"
        }
    ],
    "github": {
        "silent": true
    }
} 